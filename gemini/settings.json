{
  "context": {
    "fileName": [
      "GEMINI.md",
      "AGENTS.md",
      "CONVENTIONS.md"
    ]
  },
  "mcpServers": {
    "github.com/upstash/context7-mcp": {
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "command": "npx"
    }
  },
  "general": {
    "preferredEditor": "neovim",
    "disableAutoUpdate": true,
    "previewFeatures": true,
    "vimMode": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "ANSI"
  }
}